  "license": "ISC",
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.17",
    "mongodb": "^6.15.0",
    "tailwindcss": "^4.0.17"
  }
}
